{
	"name": "shizu",
	"version": "0.1.2",
	"type": "module",
	"private": true,
	"engines": {
		"node": ">=21.0.0"
	},
	"scripts": {
		"dev": "tsx ./src/lib/helpers/init.ts & vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview --port 3000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"init": "tsx ./src/lib/helpers/init.ts",
		"ping": "node ./src/lib/ping/ping.mjs",
		"lint": "eslint . & prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:ui": " eslint ./src/lib/components/ui & prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check ./src/lib/components/ui",
		"format": "eslint --fix . & prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"format:ui": "eslint --fix ./src/lib/components/ui/ & prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write ./src/lib/components/ui/"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tabler/icons-svelte": "^2.44.0",
		"@types/node": "^20.10.5",
		"@types/tcp-ping": "^0.1.5",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.2",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.0",
		"zod": "^3.22.4"
	},
	"dependencies": {
		"@types/lodash": "^4.14.202",
		"axios": "^1.6.2",
		"bits-ui": "^0.11.8",
		"chart.js": "^4.4.1",
		"clsx": "^2.0.0",
		"cron-validator": "^1.3.1",
		"cronstrue": "^2.47.0",
		"dayjs": "^1.11.10",
		"formsnap": "^0.4.1",
		"lodash": "^4.17.21",
		"mode-watcher": "^0.1.2",
		"nanoid": "^5.0.4",
		"radix-icons-svelte": "^1.2.1",
		"sveltekit-superforms": "^1.13.1",
		"tailwind-merge": "^2.2.0",
		"tailwind-variants": "^0.1.19",
		"tcp-ping": "^0.1.1"
	}
}